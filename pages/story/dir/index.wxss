page{
    height:100%;
    overflow-y:hidden;
}
.parent{
    position:relative;
    z-index:100;
    padding:24rpx 58rpx 0;
}

.parent .header_img{
    padding:0 46rpx;
}
.parent .header_img .img_wrap{
    display:block;
    width:100%;
    position:relative;
} 
.parent .header_img .img_wrap text{
    display: -webkit-box;
    text-overflow:ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    white-space:nowrap;
    width:376rpx;
    height:122rpx;
    font:54rpx/122rpx '微软雅黑';
    color:#000; 
    text-align:center;
    position:absolute;
    left:50%;
    transform:translateX(-50%);
    bottom:204rpx;

}
.parent .header_img .img_wrap image{
    display:block;
    width:100%;
}

.main{
   margin-top:-106rpx;
   padding:0 24rpx;
   position:relative;
}
.main .line{
    position:absolute;
    left:0;
    right:0;
    top:-10rpx;
}
.main .line image{
    display:block;
    width:100%;
}
.main .list{
    padding-right:18rpx;
}
.main .list .item{
    position:relative;
    padding:60rpx 128rpx 60rpx 178rpx;
    background:#fff;
    border-radius:30rpx;
    overflow:hidden;
    margin-bottom:38rpx;
    box-shadow:8rpx 8rpx 4rpx 8rpx #87659d;
}

.main .list .item .pro_img{
    display:block;
    width:110rpx;
    height:110rpx;
    border-radius:50%;
    position:absolute;
    left:34rpx;
    top:50%;
    transform:translateY(-50%);
}
.main .list .item .msg .title{
    display: -webkit-box;
    text-overflow:ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    height:62rpx;
    font:40rpx/62rpx '微软雅黑';
    color:#000;
}
.main .list .item .tip_icon{
    position:absolute;
    top:0;
    right:0;
    bottom:0;
}

.main .list .item .tip_icon  .star{
    position:absolute;
    right:0;
    top:22rpx;
}
.main .list .item .tip_icon  .star image {
    display:block;
    width:128rpx;
}

.main .list .item .tip_icon  .star{
    display:flex;
    align-items:center;
    box-sizing:border-box;
    position:absolute;
    top:50%;
    right:46rpx;
    margin-top:-37rpx;
} 
.main .list .item .tip_icon  .star image{
    display:block;
    width:74rpx;
    height:74rpx;
}
 
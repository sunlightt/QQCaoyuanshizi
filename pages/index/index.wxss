page{
    height:100%;
    overflow-y:hidden;
}
.page_bg {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

.page_bg image {
    display: block;
    width: 100%;
    height: 100%;
}

.float_robot {
    width:50%;
    position: fixed;
    left:0;
    bottom: -66rpx;
    z-index: 100;
}

.float_robot image {
    display: block;
    width: 102rpx;
    margin:0 auto;
}

.parent {
    position: relative;
    z-index: 100;
    padding: 24rpx 42rpx 0;
}

.parent .header .search_area {
    background: rgba(255, 255, 255, 0.5);
    border-radius: 30rpx;
}

.parent .header .search_area input {
    display: block;
    padding: 0 12rpx;
    height: 100rpx;
    font: 32rpx/100rpx '微软雅黑';
    color: #fff;
}

.parent .robot .robot_title_wrap {
    position: relative;
    padding: 60rpx 0 94rpx 262rpx;
}

.parent .robot .robot_title_wrap image {
    display: block;
    width: 166rpx;
    position: absolute;
    left: 24rpx;
    top: 50%;
    transform: translateY(-50%);
}

.parent .robot .robot_title_wrap text {
    display: block;
    font: 32rpx/44rpx '微软雅黑';
    color: #000;
    text-align: left;
}

.main {
    margin-top: -52rpx;
    padding: 0 24rpx;
    position: relative;
}

.main .line {
    position: absolute;
    left: 0;
    right: 0;
    top: -10rpx;
}

.main .line image {
    display: block;
    width: 100%;
}

.main .list .item {
    position: relative;
    padding: 30rpx 128rpx 30rpx 178rpx;
    background: #fff6ea;
    border-radius: 30rpx 0 30rpx 30rpx;
    overflow: hidden;
    margin-bottom: 20rpx;
}


.main .list .bg1{
    background:#effaff;
}

.main .list .bg2{
    background:#feeceb;
}

.main .list .bg3{
    background:#eafae3;
}
.main .list .item .pro_img {
    display: block;
    width: 110rpx;
    height: 110rpx; 
    position: absolute;
    border-radius:30rpx;
    left: 34rpx;
    top: 50%;
    /* transform: translateY(-50%); */
    margin-top:-55rpx;
}

.main .list .item .msg .title {
    display: -webkit-box;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    height: 62rpx;
    font: 46rpx/62rpx '微软雅黑';
    color: #000;
}

.main .list .item .msg .subtitle {
    display: -webkit-box;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    height: 46rpx;
    font: 30rpx/46rpx '微软雅黑';
    color: #000;
}

.main .list .item .tip_icon {
    width:178rpx;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    z-index:9999;
}

.main .list .item .tip_icon .free {
    position: absolute;
    right: 0;
    top: 0;
    overflow: hidden; 
}

.main .list .item .tip_icon .free image {
    display: block;
    width: 100rpx;
    height: 92rpx;
}

.main .list .item .tip_icon  .star, .main .list .item .tip_icon .un_complete {
    position: absolute;
    right: 0;
    top: 22rpx;
}

.main .list .item .tip_icon  .star image,
.main .list .item .tip_icon .un_complete image {
    display: block;
    width: 128rpx;
}

.main .list .item .tip_icon  .star {
    min-width: 128rpx;
    height: 50rpx;
    background: #ff8f00;
    display: flex;
    align-items: center;
    box-sizing: border-box;
    padding-left: 20rpx;
    border-radius: 26rpx 0 0 26rpx;
}

.main .list .item .tip_icon  .star text{

    display:inline-block;
    font:20rpx/50rpx '微软雅黑';
    color:#fff;
}

.main .list .item .tip_icon  .star image {
    display: block;
    width: 25rpx;
    height: 25rpx;
}

.main .list .last_item {
    padding: 64rpx 128rpx 64rpx 178rpx;
    background: rgba(255, 255, 255, 0.3);
}

.main .list .last_item .more .more_inf {
    display: block;
    font: 26rpx/38rpx '微软雅黑';
    color: #4c645c;
    text-align: center;
}
